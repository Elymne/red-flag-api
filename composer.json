{
    "name": "sacha_djurdjevic/red-flag-api",
    "description": "API REST pour communiquer de l'app mobile RedFlag vers une db pour stocker la data n√©cessaire.",
    "authors": [
        {
            "name": "Sacha Djurdjevic",
            "email": "sacha.djudjevic@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Core\\": [
                "src/core",
                "src/core/di"
            ],
            "Domain\\Models\\": [
                "src/domain/models"
            ],
            "Domain\\Repositories\\": [
                "src/domain/repositories"
            ],
            "Domain\\Gateways\\": [
                "src/domain/gateways"
            ],
            "Domain\\Usecases\\": [
                "src/domain/usecases",
                "src/domain/params"
            ],
            "Infra\\Router\\": [
                "src/infra/router",
                "src/infra/router/handlers",
                "src/infra/router/cache",
                "src/infra/router/limiter"
            ],
            "Infra\\Datasources\\": [
                "src/infra/datasources/local",
                "src/infra/datasources/local/database",
                "src/infra/datasources/remote"
            ],
            "Infra\\Env\\": [
                "src/infra/env"
            ],
            "Infra\\Uuid\\": [
                "src/infra/uuid"
            ],
            "Infra\\Logger\\": [
                "src/infra/logger"
            ]
        }
    },
    "require": {
        "ramsey/uuid": "^4.7",
        "pecee/simple-router": "*",
        "vlucas/phpdotenv": "^5.6",
        "zircote/swagger-php": "^5.1",
        "monolog/monolog": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "12",
        "mockery/mockery": "^1.6"
    }
}
